{
    "manifest_version": 2,
  
    "name": "Crypto Data",
    "description": "This extension will display cryptocurrency data.",
    "version": "1.0.1",
    "icons": {
        "128": "logo.png"
      },
  
    "browser_action": {
     "default_icon": "logo.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
  }